{
  "name": "freeman",
  "productName": "FreeMAN",
  "version": "0.7.2",
  "description": "A free, extensible, cross-platform file manager for power users",
  "main": "app/main.js",
  "build": {
    "appId": "com.electron.freeman",
    "copyright": "Copyright Â© 2018 Matthew James",
    "linux": {
      "category": "System"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/matthew-matvei/freeman.git"
  },
  "private": true,
  "keywords": [
    "File Manager",
    "Power User"
  ],
  "author": "Matthew James",
  "license": "GPL3",
  "bugs": {
    "url": "https://github.com/matthew-matvei/freeman/issues"
