  "scripts": {
    "build": "webpack --config webpack.common.js",
    "prebuild:test": "rimraf __tests__/*",
    "build:test": "webpack --config webpack.test.js",
    "prebuild:prod": "concurrently \"rimraf app/font/\" \"rimraf app/*.woff2\" \"rimraf app/*.css\" \"rimraf app/*.js\"",
    "build:prod": "webpack --config webpack.prod.js",
    "pretest": "npm run build:test",
    "test": "xvfb-maybe electron-mocha --renderer __tests__",
    "prestart": "npm run build",
    "start": "electron app/main.js",
    "prepackage": "concurrently \"rimraf dist\" \"npm run build:prod\"",
    "package": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "lint": "tslint --project tsconfig.json --fix"
  },
  "homepage": "https://github.com/matthew-matvei/freeman#readme",
